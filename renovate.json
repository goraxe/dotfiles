{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended" ],
  "ignorePaths": [ "**/.asdf/**" ],
  "packageRules": [
    {
      "groupName": "asdf",
      "matchManagers": ["asdf"]
    },
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest",
        "pin",
        "pinDigest",
        "lockFileMaintenance"
      ],
      "automerge": true
    }
  ]
}
